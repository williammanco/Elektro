{
  "name": "Elektro",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Audiovisive experience first time made",
  "homepage": "https://williamman.co",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dev:wds",
    "build": "yarn prod:build",
    "dev:wds": "webpack-dashboard -- webpack-dev-server --progress",
    "prod:build": "rimraf lib && babel src -d lib --ignore .test.js && BABEL_ENV=production webpack --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "start:documentation": "documentation serve --watch --shallow src/**",
    "build:documentation": "documentation build src/** -f html -o docs --shallow",
    "build:readme": "documentation readme src/** md --shallow --section=API",
    "test": "eslint src && flow && jest --coverage",
    "precommit-": "yarn test",
    "prepush-": "yarn test"
  },
  "dependencies": {
    "gsap": "^1.19.1",
    "improved-noise": "^0.0.3",
    "jquery": "^3.2.1",
    "three": "0.84",
    "web-audio-analyser": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-flow": "^6.23.0",
    "css-loader": "^0.28.0",
    "documentation": "^4.0.0-beta.19",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-react": "^7.0.0",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.45.0",
    "glslify": "^6.0.2",
    "husky": "^0.13.2",
    "imports-loader": "^0.7.1",
    "jest": "^20.0.0",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "three-orbitcontrols": "^1.1.2",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.2"
  }
}
